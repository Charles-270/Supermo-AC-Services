{
  "name": "supremo-ac-platform",
  "private": true,
  "version": "0.1.0",
  "description": "Digital transformation platform for Supremo AC Services - HVAC service provider in Ghana",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "build:analyze": "tsc -b && vite build --mode analyze",
    "lint": "eslint .",
    "preview": "vite preview",
    "test": "vitest",
    "perf:check": "npm run build && npm run preview",
    "mcp:check": "bash scripts/mcp-health-check.sh",
