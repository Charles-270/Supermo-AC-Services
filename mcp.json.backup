{
  "mcpServers": {
    "chrome-devtools": {
      "command": "cmd",
      "args": [
        "/c",
        "npx",
        "-y",
        "chrome-devtools-mcp@latest",
        "--chrome-port",
        "9222"
      ]
    },
    "shadcn": {
      "command": "npx",
      "args": [
        "-y",
        "mcp-remote",
        "https://www.shadcn.io/api/mcp"
      ]
    }
  }
}